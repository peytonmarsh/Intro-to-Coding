// >  greater than
// <  less than
// ===  equal to. 5 === 5 would be true.
// <=  less than or equal to 
// >=  greater than or equal to
// !==  not equal to. 4 !== 5 would be true.

// Write a program that prints a message to the screen based on a users age and country.
// Feel free to change these variables or create new ones so you can test all cases.

const age = 33
const country = 'USA'

if(age < 16) {
   console.log("You're not old enough to do anything yet.")
}   else if (age >= 16 && age < 18) {
   console.log("Be careful driving.")
}   else if (age >= 16 && age <= 18 && country === "USA") {
   console.log("Go Vote!")
}   else if (age >= 16 && <= 18 && country !== "USA") {
   console.log("You can probably have some wine.")
}    else {
   console.log("You're old enough to figure it out for yourself.")
}
